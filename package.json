{
  "name": "adventuretron",
  "version": "0.0.16",
  "description": "Create self-guided workshops using electron",
  "main": "main.js",
  "browser": "renderer/index.js",
  "style": "renderer/css/bundle.css",
  "scripts": {
    "test": "tape tests/*.js",
    "build": "browserify renderer/index.js -t sheetify/transform -p [ css-extract -o renderer/css/bundle.css ] -o /dev/null",
    "watch": "watchify renderer/index.js -t sheetify/transform -p [ css-extract -o renderer/css/bundle.css ] -o /dev/null",
    "prepublish": "npm run build"
  },
  "bin": {
    "adventuretron": "bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adventuretron/adventuretron.git"
  },
  "keywords": [
    "nodeschool",
    "workshopper",
    "adventure",
    "electron"
  ],
  "author": "sethvincent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adventuretron/adventuretron/issues"
  },
  "homepage": "https://github.com/adventuretron/adventuretron#readme",
  "devDependencies": {
    "browserify": "^13.1.0",
    "css-extract": "^1.1.3",
    "electron-packager": "^8.1.0",
    "standard": "^8.3.0",
    "tape": "^4.6.2",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "beldown": "^1.1.0",
    "bulk-require": "^1.0.0",
    "choo": "^3.3.0",
    "choo-log": "^1.4.1",
    "create-html": "^1.1.0",
    "dom-css": "^2.1.0",
    "domify": "^1.4.0",
    "each-async": "^1.1.1",
    "electron": "^1.4.3",
    "execspawn": "^1.0.1",
    "glob": "^7.1.0",
    "highlight.js": "^9.7.0",
    "insert-css": "^1.0.0",
    "interactive-sandbox": "^1.1.0",
    "level": "^1.4.0",
    "marked": "^0.3.6",
    "maxstache-stream": "^1.0.4",
    "sheetify": "^5.1.1",
    "subcommand": "^2.0.3",
    "tachyons": "^4.5.3",
    "tar-fs": "^1.13.2",
    "xtend": "^4.0.1"
  }
}
